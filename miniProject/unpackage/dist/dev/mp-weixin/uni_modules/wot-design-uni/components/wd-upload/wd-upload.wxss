.wot-theme-dark .wd-upload__evoke.data-v-d50d9cde {
  background-color: var(--wot-dark-background4, #323233);
  color: var(--wot-dark-color3, rgba(232, 230, 227, 0.8));
}
.wot-theme-dark .wd-upload__evoke.is-disabled.data-v-d50d9cde {
  color: var(--wot-dark-color-gray, var(--wot-color-secondary, #595959));
}
.wot-theme-dark .wd-upload__file.data-v-d50d9cde {
  background-color: var(--wot-dark-background4, #323233);
}
.wot-theme-dark .wd-upload__file-name.data-v-d50d9cde {
  color: var(--wot-dark-color3, rgba(232, 230, 227, 0.8));
}
.wd-upload.data-v-d50d9cde {
  position: relative;
  display: flex;
  flex-wrap: wrap;
}
.wd-upload__evoke.data-v-d50d9cde {
  position: relative;
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: var(--wot-upload-size, 80px);
  height: var(--wot-upload-size, 80px);
  font-size: var(--wot-upload-evoke-icon-size, 32px);
  background-color: var(--wot-upload-evoke-bg, rgba(0, 0, 0, 0.04));
  color: var(--wot-upload-evoke-color, rgba(0, 0, 0, 0.25));
  margin-bottom: 12px;
}
.wd-upload__evoke.is-disabled.data-v-d50d9cde {
  color: var(--wot-upload-evoke-disabled-color, rgba(0, 0, 0, 0.09));
}
.wd-upload__evoke-num.data-v-d50d9cde {
  font-size: 14px;
  line-height: 14px;
  margin-top: 8px;
}
.data-v-d50d9cde  .wd-upload__evoke-icon {
  width: 32px;
  height: 32px;
}
.wd-upload__input.data-v-d50d9cde {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  opacity: 0;
}
.wd-upload__preview.data-v-d50d9cde {
  position: relative;
  width: var(--wot-upload-size, 80px);
  height: var(--wot-upload-size, 80px);
  margin: 0 12px 12px 0;
}
.wd-upload__preview-list.data-v-d50d9cde {
  display: flex;
}
.wd-upload__picture.data-v-d50d9cde, .wd-upload__file.data-v-d50d9cde, .wd-upload__video.data-v-d50d9cde {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
}
.wd-upload__file.data-v-d50d9cde, .wd-upload__video.data-v-d50d9cde {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: var(--wot-upload-evoke-bg, rgba(0, 0, 0, 0.04));
}
.wd-upload__video-icon.data-v-d50d9cde, .wd-upload__file-icon.data-v-d50d9cde {
  font-size: var(--wot-upload-cover-icon-size, 22px);
}
.wd-upload__file-name.data-v-d50d9cde, .wd-upload__video-name.data-v-d50d9cde {
  width: 100%;
  font-size: var(--wot-upload-file-fs, 12px);
  color: var(--wot-upload-file-color, var(--wot-color-secondary, #595959));
  box-sizing: border-box;
  padding: 0 4px;
  text-align: center;
  margin-top: 8px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.data-v-d50d9cde  .wd-upload__video-paly {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 24px;
  color: var(--wot-color-white, white);
}
.data-v-d50d9cde  .wd-upload__video-paly::before {
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 50%;
}
.data-v-d50d9cde  .wd-upload__close {
  position: absolute;
  right: calc(var(--wot-upload-close-icon-size, 16px) / 2 * -1);
  top: calc(var(--wot-upload-close-icon-size, 16px) / 2 * -1);
  font-size: var(--wot-upload-close-icon-size, 16px);
  z-index: 1;
  color: var(--wot-upload-close-icon-color, rgba(0, 0, 0, 0.65));
  width: var(--wot-upload-close-icon-size, 16px);
  height: var(--wot-upload-close-icon-size, 16px);
  line-height: var(--wot-upload-close-icon-size, 16px);
}
.data-v-d50d9cde  .wd-upload__close::after {
  position: absolute;
  content: "";
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background-color: var(--wot-color-white, white);
  left: 0;
  z-index: -1;
}
.wd-upload__mask.data-v-d50d9cde {
  position: absolute;
  top: 0;
  left: 0;
  background-color: var(--wot-upload-preview-name-bg, rgba(0, 0, 0, 0.6));
}
.wd-upload__progress-txt.data-v-d50d9cde {
  font-size: var(--wot-upload-progress-fs, 14px);
  line-height: var(--wot-upload-progress-fs, 14px);
  margin-top: 9px;
  color: var(--wot-color-white, white);
}
.data-v-d50d9cde  .wd-upload__icon {
  font-size: var(--wot-upload-preview-icon-size, 24px);
  color: var(--wot-color-white, white);
}
.wd-upload__status-content.data-v-d50d9cde {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}